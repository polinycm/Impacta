@model EPT.Models.Contratante

@{
    ViewData["Title"] = "Excluir";
}
<div class="col-1 mb-4">
    <nav class="navbar navbar-expand-lg">
        <button type="button" id="sidebarCollapse" class="btn btn-info">
            <i class="fas fa-align-left"></i>
        </button>
    </nav>
</div>
<br />
<div class="alterar">
    <h3>Confirmar exclusão deste contratante? </h3>
</div>
<div class="alterar">
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            ID
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ContratanteId)
        </dd>
        <dt class = "col-sm-2">
            Contratante
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.contratante)
        </dd>
    </dl>
    <br />
    <div class="row">
        <div style="margin-right:5px">
            <form asp-action="Excluir">
                <input asp-route-contratanteId="@Model.ContratanteId" type="submit" value="Excluir" class="btn btn-success" /> |
                <a href="@Url.Action("Index")" class="btn btn-danger">Voltar</a>

            </form>

        </div>

    </div>

</div>
